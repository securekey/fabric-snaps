{
  "Deadline": "5m",
  "EnableGC": true,
  "Exclude": [
    "bddtests/...",
    "^(.*_test.go.*$)$"
  ],
  "WarnUnmatchedDirective": true,
  "Vendor": true,
  "Enable": [
    "deadcode",
    "gocyclo",
    "gofmt",
    "gotype",
    "goimports",
    "golint",
    "gosimple",
    "ineffassign",
    "interfacer",
    "misspell",
    "unconvert",
    "unused",
    "vet",
    "varcheck",
    "maligned",
    "errcheck",
    "megacheck",
    "goconst",
    "gas",
    "structcheck"
  ],
  "Linters": {
    "gas": {
      "Command": "gas -fmt=csv",
      "Pattern": "^(?P<path>.*?\\.go),(?P<line>\\d+)(-\\d+)?,(?P<message>[^,]+,[^,]+,[^,]+)",
      "InstallFrom": "github.com/GoASTScanner/gas",
      "PartitionStrategy": "packages",
      "defaultEnabled": true,
      "IsFast": true
    }
  },
  "Cyclo": 10,
  "Aggregate": true
}
